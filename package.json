{
  "name": "eventuate",
  "version": "0.0.13",
  "description": "Extracts information from parkrun results pages for inclusion in reports.",
  "keywords": [
    "parkrun",
    "firefox",
    "web",
    "extension"
  ],
  "homepage": "https://github.com/johnsyweb/eventuate#readme",
  "bugs": {
    "url": "https://github.com/johnsyweb/eventuate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnsyweb/eventuate.git"
  },
  "license": "MIT",
  "author": "Pete Johns",
  "scripts": {
    "build": "webpack",
    "package": "pnpm lint && pnpm build && pnpm web-ext:lint && pnpm web-ext:build && pnpm repack-for-chromium",
    "repack-for-chromium": "bash './script/repack-for-chromium'",
    "start": "web-ext run -v -u https://www.parkrun.com.au/brimbank/results/latestresults/",
    "test": "jest",
    "lint": "eslint .",
    "web-ext:build": "web-ext build -o",
    "web-ext:lint": "web-ext lint"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.16.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "web-ext": "^8.3.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "pnpm@9.12.2+sha512.22721b3a11f81661ae1ec68ce1a7b879425a1ca5b991c975b074ac220b187ce56c708fe5db69f4c962c989452eee76c82877f4ee80f474cebd61ee13461b6228",
  "webExt": {
    "verbose": true,
    "ignoreFiles": [
      ".tool-versions",
      ".web-extension-id",
      "CODE_OF_CONDUCT.md",
      "Dockerfile",
      "LICENSE.txt",
      "README.md",
      "amo.json",
      "assets",
      "chromium*",
      "eslint.config.mjs",
      "jest.config.ts",
      "package.json",
      "pnpm-lock.yaml",
      "script",
      "src",
      "target",
      "test",
      "webpack.config.js",
      "tsconfig.json"
    ]
  }
}
