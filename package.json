{
  "name": "eventuate",
  "version": "0.0.11",
  "description": "Extracts information from parkrun results pages for inclusion in reports.",
  "keywords": [
    "parkrun",
    "firefox",
    "web",
    "extension"
  ],
  "homepage": "https://github.com/johnsyweb/eventuate#readme",
  "bugs": {
    "url": "https://github.com/johnsyweb/eventuate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnsyweb/eventuate.git"
  },
  "license": "MIT",
  "author": "Pete Johns",
  "scripts": {
    "build": "webpack",
    "package": "pnpm lint && pnpm build && pnpm web-ext:lint && pnpm web-ext:build && pnpm repack-for-chromium",
    "repack-for-chromium": "bash './script/repack-for-chromium'",
    "start": "web-ext run -v -u https://www.parkrun.com.au/brimbank/results/latestresults/",
    "test": "jest",
    "lint": "eslint .",
    "web-ext:build": "web-ext build -o",
    "web-ext:lint": "web-ext lint"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.7",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^9.13.0",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "web-ext": "^8.3.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "packageManager": "pnpm@9.2.0",
  "webExt": {
    "verbose": true,
    "ignoreFiles": [
      ".web-extension-id",
      "Dockerfile",
      "CODE_OF_CONDUCT.md",
      "LICENSE.txt",
      "README.md",
      "chromium*",
      "jest.config.ts",
      "package.json",
      "pnpm-lock.yaml",
      "script",
      "src",
      "target",
      "test",
      "tsconfig.json"
    ]
  }
}
